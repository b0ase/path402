{
  "name": "@path402/core",
  "version": "4.0.0-alpha.2",
  "description": "Core business logic for path402 - wallet, gossip, token minting, database",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./db": {
      "types": "./dist/db/index.d.ts",
      "import": "./dist/db/index.js",
      "require": "./dist/db/index.js"
    },
    "./wallet": {
      "types": "./dist/wallet/index.d.ts",
      "import": "./dist/wallet/index.js",
      "require": "./dist/wallet/index.js"
    },
    "./token": {
      "types": "./dist/token/mint.d.ts",
      "import": "./dist/token/mint.js",
      "require": "./dist/token/mint.js"
    },
    "./gossip": {
      "types": "./dist/gossip/index.d.ts",
      "import": "./dist/gossip/index.js",
      "require": "./dist/gossip/index.js"
    },
    "./content": {
      "types": "./dist/content/index.d.ts",
      "import": "./dist/content/index.js",
      "require": "./dist/content/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.js",
      "require": "./dist/services/index.js"
    },
    "./mcp": {
      "types": "./dist/mcp.d.ts",
      "import": "./dist/mcp.js",
      "require": "./dist/mcp.js"
    },
    "./publish": {
      "types": "./dist/publish/index.d.ts",
      "import": "./dist/publish/index.js",
      "require": "./dist/publish/index.js"
    }
  },
  "scripts": {
    "build": "tsc && cp src/db/schema.sql dist/db/schema.sql",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@babbage/sdk": "^0.4.48",
    "@bsv/auth-express-middleware": "^2.0.2",
    "@bsv/payment-express-middleware": "^2.0.0",
    "@bsv/sdk": "^2.0.1",
    "@libp2p/bootstrap": "^10.0.0",
    "@libp2p/gossipsub": "^15.0.12",
    "@libp2p/identify": "^4.0.10",
    "@libp2p/kad-dht": "^16.1.3",
    "@libp2p/mdns": "^12.0.11",
    "@libp2p/noise": "^1.0.1",
    "@libp2p/peer-id": "^6.0.4",
    "@libp2p/peer-id-factory": "^4.2.4",
    "@libp2p/tcp": "^11.0.10",
    "@libp2p/yamux": "^8.0.1",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@multiformats/multiaddr": "^12.2.1",
    "@multiformats/multiaddr-matcher": "^3.0.1",
    "@supabase/supabase-js": "^2.94.0",
    "better-sqlite3": "^11.10.0",
    "bsv": "^2.0.10",
    "express": "^4.21.0",
    "libp2p": "^1.9.4",
    "zod": "^3.24.0"
  },
  "optionalDependencies": {
    "@path402/htm": "workspace:*"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  }
}