{
  "name": "@b0ase/path402-types",
  "version": "4.0.0-alpha.1",
  "description": "Shared TypeScript types for the path402 token exchange protocol",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./exchange": {
      "types": "./dist/exchange.d.ts",
      "import": "./dist/exchange.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "typescript": "^5.7.0"
  }
}
